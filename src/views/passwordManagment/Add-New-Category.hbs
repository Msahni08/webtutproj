        <div class="addcatInput">
           
            <form method="post" action="AddNewCategory">
                <p>Add New Password Category</p>
                {{#if this.errors}}
                    <div style="color: red; margin:5px;font-size:20px;">
                        <strong>!{{errors.passcat.msg}}</strong>
                   </div>
                {{/if}}
                  {{#if this.success}}
                    <div style="color: blue; margin:5px;font-size:20px;">
                        <strong>!{{success}}</strong>
                   </div>
                {{/if}}
               <label for="User Name">User Name</label>  <input type="text" placeholder="Enter Password Category Name" name="passcat">
               <button type="submit">Add Category</button>
             </form>  

             <a href="#">View All Category List</a>
