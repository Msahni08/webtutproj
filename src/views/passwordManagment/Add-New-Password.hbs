{{>passwordCategory_Nav}}
<div style="text-align: right; margin:1% 15% 1% 0% ; color:blue">Logged as :{{logMob}}  </div>

                
        <div class="addPss">
           {{#if this.success}}
                    <div style="color:pink; top-margin:30%;font-size:20px; bolder;">
                        <strong>!{{success}}</strong>
                   </div>
                   {{/if}} 
            <form method="POST" action="Add-New-Password">
                <p>Add New Password</p>
               <label for="User Name">User Name</label>
                <select class="selectpass" name="addPassword" id="addPassword">
                  {{#each data}}
                   <option value="{{this.passcatb}}">{{this.passcatb}}</option>
                  {{/each}}
                   
                </select>
                <label for="proj"id="paslabel">Project Name</label>
                <input type="text" class="projname" name="projtname">
                <label id="passlabel" for="Pass_details">Password Details</label>
                <div class="txt">
                  <textarea  rows="5"placeholder="Enter Your Password Details" id="editor" name='editor'></textarea>
                </div>
               <button type="submit">Add Password</button>
             </form>  

             <a href="#">View All Password List</a>
        </div>
