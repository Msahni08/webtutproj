{{>joints}}
 <h3 style="text-align: right;margin-right:20px;color:red"><img src="/profileImg/{{propic}} " alt="img" style="width: 50px; height:50px;border-radius: 3rem;">Logged as:{{logMob}}</h3> 


<table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>AddPassword</th>
                <th>Projname</th>
                <th>Editor</th>
                <th>Date</th>
                <th>Join_Id</th>
                <th>Join_passcatb</th>
                <th>Join_Date</th>
                <th>Action</th>

                
            </tr>
        </thead>
        <tbody>
          {{#each joinsDetails}}
              <tr>
                <td> {{this._id}}</td>
                <td> {{this.addPassword}}</td>
                <td> {{this.projtname}}</td>
                <td> {{this.editor}}</td>
                <td> {{this.Date}}</td>
                <td> {{this.pass_cat_details._id}}</td>
                <td> {{this.pass_cat_details.passcatb}}</td>
                <td> {{this.pass_cat_details.Date}}</td>
                <td><a class ="btn btn-primary btn-ls"href="/joints/join_edit/{{this._id}}"><i class=" fa fa-edit"></i></a>
                <a class ="btn btn-danger btn-ls"href="/passmanagement/join_delete/{{this._id}}"><i class=" fa fa-trash"></i></a>

            </tr>
            {{/each}}
        </tbody>
</table>
